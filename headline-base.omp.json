{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "secondary_prompt": {
    "template": "$ ",
    "foreground_templates": [
      "{{ if gt .Code 0 }}red{{ end }}",
      "default"
    ]
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<b><o>\uf007{{ .UserName }}</o></b>",
          "type": "session",
          "style": "plain",
          "foreground_templates": [
            "{{ if .SSHSession }}magenta{{ end }}",
            "{{ if .WSL }}:lightMagenta{{ end }}",
            "red"
          ]
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<o>@</o>",
          "foreground": "darkGray",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<b><o>\uf6c4{{ .HostName }}</o></b><darkGray><o>: </o></>",
          "foreground": "yellow",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_separator_icon": "/",
            "style": "full"
          },
          "template": "<b><o>\ue5fe {{ .Path }}</o></b>",
          "foreground": "blue",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "branch_icon": "\ue725 ",
            "cache_duration": "none"
          },
          "template": "<darkGray><o> </o></><b><o>{{ url .HEAD .UpstreamURL }}</o></b>",
          "foreground": "cyan",
          "type": "git",
          "style": "accordion"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true
          },
          "template": "{{ if or .Staging.Changed (lt .Staging.Modified .Working.Modified) (and .Working.Changed (not .Staging.Modified)) .Ahead .Behind (gt .StashCount 0) }}<o> [</o>{{ end }}",
          "foreground": "darkGray",
          "type": "git",
          "style": "accordion"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_status": true
          },
          "template": "{{ if or .Staging.Changed (lt .Staging.Modified .Working.Modified) (and .Working.Changed (not .Staging.Modified)) }}<b><o>{{ if .Staging.Changed }}+{{ end }}{{ if lt .Staging.Modified .Working.Modified }}!{{ end }}{{ if and .Working.Changed (not .Staging.Modified) }}?{{ end }}</o></b>{{ end }}",
          "foreground": "magenta",
          "type": "git",
          "style": "accordion"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_status": true
          },
          "template": "{{ if or .Ahead .Behind }}<b><o>{{ if gt .Behind 0 }}\u2193{{ end }}{{ if gt .Ahead 0 }}\u2191{{ end }}{{ if eq .Behind .Ahead }}\u2195{{ end }}</o></b>{{ end }}",
          "foreground": "magenta",
          "type": "git",
          "style": "accordion"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true
          },
          "template": "{{ if gt .StashCount 0 }}<b><o>*</o></b>{{ end }}",
          "foreground": "magenta",
          "type": "git",
          "style": "accordion"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true
          },
          "template": "{{ if or .Staging.Changed (lt .Staging.Modified .Working.Modified) (and .Working.Changed (not .Staging.Modified)) .Ahead .Behind (gt .StashCount 0) }}<o>] </o>{{ end }}",
          "foreground": "darkGray",
          "type": "git",
          "style": "accordion"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "filler": "<darkGray><o> </o></>",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<o> </o>",
          "foreground": "darkGray",
          "type": "text",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "$ ",
          "type": "text",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}red{{ end }}",
            "default"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
